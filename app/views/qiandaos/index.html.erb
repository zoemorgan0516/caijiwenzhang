<p id="notice"><%= notice %></p>

<h1>Qiandaos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qiandaos.each do |qiandao| %>
      <tr>
        <td><%= link_to 'Show', qiandao %></td>
        <td><%= link_to 'Edit', edit_qiandao_path(qiandao) %></td>
        <td><%= link_to 'Destroy', qiandao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qiandao', new_qiandao_path %>
